generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id       String @id @default(uuid())
  username String @unique
  password String
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String   @db.Text  // MediumText may be needed for MySql
  expiresAt   DateTime
}
